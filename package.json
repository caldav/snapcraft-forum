{
  "name": "snapcraft-forum",
  "productName": "Snapcraft Forum",
  "version": "1.0.2",
  "description": "Official Snapcraft forum desktop app",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . app --platform linux --arch x64 --out dist/ --icon=resources/icon.png",
    "deb64": "electron-installer-debian --src dist/app-linux-x64/ --arch amd64 --config deb_config/config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryanleesipes/snapcraft-forum.git"
  },
  "keywords": [
    "snappy",
    "snapcraft",
    "electron-app"
  ],
  "author": "Ryan Sipes, ryan@sipes.us",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryanleesipes/snapcraft-forum/issues"
  },
  "homepage": "https://snapcraft.io",
  "devDependencies": {
    "electron-installer-debian": "latest",
    "electron-packager": "latest",
    "electron": "latest",
    "electron-window-state": "latest"
  },
  "dependencies": {
    "electron-window-state": "latest"
  }
}
